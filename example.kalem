#import <iostream>
#import <string>

@my_function void {
	@print "Hi, from Kalem!\n"
}

@main int {
	string test = "Hello, Kalem!\n"

	int _test = 10
	unsign _test_ = 20

	@print "String: "
	@print test

	@print "\nInteger: "
	@print _test

	@print "\nUnsigned Integer: "
	@print _test_

	@print "\n"

	@my_function

	@return 0
}
